{
  "name": "nine-tails",
  "version": "0.9.4",
  "description": "A handy library to dynamically change css styles at run time.",
  "main": "js/nine-tails.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "gulp bundle",
    "test": "istanbul cover node_modules/tape/bin/tape -- test/*.spec.js | tap-dot && npm run remap-coverage",
    "remap-coverage": "remap-istanbul -i coverage/coverage.json --output coverage/report --type html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesrichford/nine-tails.git"
  },
  "keywords": [
    "color",
    "style",
    "css",
    "dynamic",
    "javascript"
  ],
  "author": "James Richford",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesrichford/nine-tails/issues"
  },
  "homepage": "https://github.com/jamesrichford/nine-tails",
  "devDependencies": {
    "browserify": "^13.0.0",
    "gulp": "^3.9.0",
    "gulp-live-server": "0.0.29",
    "gulp-sass": "^2.1.0",
    "gulp-typescript": "^2.8.1",
    "istanbul": "^0.4.3",
    "remap-istanbul": "^0.6.3",
    "tap-dot": "^1.0.5",
    "tape": "^4.5.1",
    "typescript": "^1.6.2",
    "typings": "^0.8.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "node-static": "^0.7.7",
    "backbone.marionette": "^2.4.4",
    "requirejs": "^2.1.22",
    "jquery": "^2.1.4"
  }
}
